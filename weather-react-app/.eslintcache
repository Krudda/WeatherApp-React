= {styles.input}\n                    type = 'text' \n                    placeholder = 'search city'\n                />\n                <Button \n                    type_view = 'type_0110'  \n                    handler = {handler} \n                    type='submit' \n                    add_class = 'button-search'\n                    >\n                        search\n                    </Button>\n                <div className = {styles.button_mic}></div>\n            </form>\n        </div>\n    )\n}\n\nexport default  SearchBlock;","/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/components/Main/index.js",["173","174","175"],"import {useSelector, useDispatch}  from 'react-redux';\n\nimport MapBlock from './MapBlock/MapBlock';\nimport WeatherBlock from './WeatherBlock/WeatherBlock';\nimport {getWeather} from '../../redux/actions';\nimport store from '../../redux/store';\n\nimport style from './main.module.scss';\n\n\nconst Main = () => {\n\n    // const state = store.getState();\n    // const weather = state.weather.weather.data;\n    // const tempDimension = state.tempDimension.tempDimension;\n    // const location = state.userLocation.userLocation;\n\n    const weather = useSelector(state => state.weather.weather.data);\n    // const city = useSelector(state => state.weather.weather.city_name);\n    const tempDimension = useSelector(state => state.tempDimension.tempDimension);\n    const location = useSelector(state => state.userLocation.userLocation);\n\n    // const dispatch = useDispatch();\n    // dispatch(getWeather(location));\n\n    // const dispatch = useDispatch();  \n    // dispatch(getWeather(location, tempDimension));\n\n    \n    return (\n        <div className = {style.main}>\n            {location && weather &&\n                <>\n                    <WeatherBlock \n                        weather = {weather}  \n                        location = {location} \n                        tempDimension = {tempDimension}\n                    />\n                    <MapBlock />\n                </>\n            }\n        </div>\n    )\n}\n\nexport default  Main;","/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/components/Main/MapBlock/MapBlock.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/components/Main/WeatherBlock/WeatherBlock.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/components/RSS/index.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/redux/rootReducer.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/redux/locationReducer.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/redux/types.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/services/getUserLocation.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/data/apiKeys.js",[],"/Users/dmitrijkruglov/Desktop/JS/React/ReactMera/WeatherApp-React/weather-react-app/src/redux/actions.js",["176"],"import {\n    SET_MAIN_BACKGROUND, \n    GET_USER_LOCATION, \n    GET_WEATHER, \n    SET_SEARCH_LOCATION, \n    ENABLE_BUTTON, \n    DISABLE_BUTTON,\n    SET_TEMP_DIMENSION,\n    SHOW_SPINNER,\n    HIDE_SPINNER\n} from './types';\n\nimport getUserLocation from '../services/getUserLocation';\nimport getMainBackground from '../services/getMainBackground';\nimport getCurrentWeather from '../services/getWeather';\nimport speakerHandler from '../services/speakerHandler';\nimport getMap from '../services/getMap'\n\nexport function setSearchLocation(location) {\n    return {\n        type: SET_SEARCH_LOCATION, \n        payload: {location}\n    }\n}\n\nexport function getRealUserLocation() {\n    return async dispatch => {\n        const userLocation = await getU